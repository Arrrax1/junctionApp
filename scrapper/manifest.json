{
  "manifest_version": 2,
  "name": "PenTec Assistant extension",
  "version": "1.0.0",
  "description": "Description of PenTec Assistant extension.",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons":{
    "16":"PenTec.png",
    "32":"PenTec.png",
    "48":"PenTec.png",
    "128":"PenTec.png"
  },
  "browser_action": {
    "default_icon": "PenTec.png",
    "default_popup": "popup.html"
  }
}
