{
    "manifest_version": 3,
    "name": "KYO Assist",
    "description": "KYO Assist, your Personal Virtual Assistant using advanced Artificial Intelegence Algorithms.",
    "version": "1.0",
    "icons": {
        "16": "public/images/kyo.png",
        "32": "public/images/kyo.png",
        "48": "public/images/kyo.png",
        "128": "public/images/kyo.png"
      },
    "action": {
        "default_popup": "index.html",
        "default_title":"KYO Assit",
        "default_icon": {
            "16": "public/images/kyo.png",
            "32": "public/images/kyo.png",
            "48": "public/images/kyo.png",
            "128": "public/images/kyo.png"
        },
        "permissions": ["activeTab", "scripting"],
        "background": {
            "service_worker": "background.js"
        }
      }
}