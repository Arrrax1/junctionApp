{
  "manifest_version": 2,
  "name": "KYO Assist",
  "version": "1.0.0",
  "description": "KYO Assist, your Personal Virtual Assistant using advanced Artificial Intelegence Algorithms.",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons":{
    "16":"images/kyo.png",
    "32":"images/kyo.png",
    "48":"images/kyo.png",
    "128":"images/kyo.png"
  },
  "browser_action": {
    "default_icon": "images/kyo.png",
    "default_popup": "index.html"
  }
}
