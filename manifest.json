{
  "manifest_version": 2,
  "name": "PenTec Assist",
  "version": "1.2.1",
  "description": "PenTec Assist, your Personal Virtual Assistant using advanced Artificial Intelegence Algorithms.",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons":{
    "16":"images/PenTec.png",
    "32":"images/PenTec.png",
    "48":"images/PenTec.png",
    "128":"images/PenTec.png"
  },
  "browser_action": {
    "default_icon": "images/PenTec.png",
    "default_popup": "index.html"
  }
}
